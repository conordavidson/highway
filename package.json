{
  "name": "highway",
  "version": "0.0.1",
  "description": "Honk Server",
  "main": "index.js",
  "author": "Conor Davidson",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"NODE_PATH=dist/ nodemon -q dist/index.js\"",
    "db:migrate": "prisma migrate dev --schema src/db/schema.prisma",
    "db:pull": "prisma db pull --schema src/db/schema.prisma",
    "db:seed": "tsc && NODE_PATH=dist/ node dist/db/seed.js",
    "db:prime": "tsc && NODE_PATH=dist/ node dist/db/prime.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "5.0.0-beta.1",
    "twilio": "^4.15.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.1",
    "babel-jest": "^29.6.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6"
  }
}
